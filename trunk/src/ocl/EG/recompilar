g++  -o eg utils.h utils.c parser.h parser.c gramatica.h gramatica.c eg_opencl.h eg_opencl.c main.c -lOpenCL -I ./include -I /usr/include -lm -Wno-pointer-arith -Wno-write-strings -D TAMANHO_POPULACAO=500 -D NUMERO_DE_GERACOES=50 -D DIMENSOES_PROBLEMA=30 -D TAMANHO_VALOR=8 -D TAXA_DE_MUTACAO=0.01 -D TAXA_DE_RECOMBINACAO=0.7 -D TAMANHO_TORNEIO=6 -D ELITE=2 -D PROFILING -D gpu
./eg > logExecucao.txt

cat logExecucao.txt
